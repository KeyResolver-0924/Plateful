# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key-here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-app-id

# Optional: Google Services Configuration
# GOOGLE_SERVICES_ANDROID_CLIENT_ID=your-android-client-id
# GOOGLE_SERVICES_IOS_CLIENT_ID=your-ios-client-id

# Optional: App Configuration
# EXPO_PUBLIC_APP_ENV=development
# EXPO_PUBLIC_API_URL=https://your-api-url.com 